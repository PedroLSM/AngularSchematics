{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "hello": {
      "description": "Create a simple custom schematic.",
      "factory": "./hello/index#hello",
      "schema": "./hello/schema.json"
    },
    "navigation": {
      "description": "Create a navigation custom schematic.",
      "factory": "./navigation/index#navigation",
      "schema": "./navigation/schema.json"
    },
    "ng-add": {
      "factory": "./ng-add/index",
      "description": "Add a simple custom schematic.",
      "schema": "./hello/schema.json"
    },
    "custom-packages": {
      "description": "A blank schematic.",
      "factory": "./custom-packages/index#customPackages"
    }
  }
}
